ncurses.c:
	delete(): backwards motions
	only syntax highlight what's on screen?
	:e autocomplete?
	change tabs:
		int ntabs = curpos / 8;
		mov(ntabs * 8 + 8)

motions.c:
	'f' and 't'

command.c:
	filters/pipes:
		:rw!cmd # filter through prog
	regex search

regex:
	simple (vi(1) - see '/'): ^$.[]*
